generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}
model product{
  id Int @id @default(autoincrement())
  ProductName String 
  ProductDescription String
  catagoryId Int
  image String
  catagory catagories @relation(fields: [catagoryId],references: [id])
}
model catagories{
  id Int @id @default(autoincrement())
  name String @unique
  url String
  products product[]
}

